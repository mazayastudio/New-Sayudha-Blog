        <script expr:src="data:assetBase + &quot;/js/vendor/jquery-3.6.0.min.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/bootstrap.min.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/isotope.pkgd.min.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/imagesloaded.pkgd.min.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/jquery.magnific-popup.min.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/jquery.marquee.min.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/imageRevealHover.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/swiper-bundle.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/TweenMax.min.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/slick.min.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/wow.min.js&quot;"></script>
        <script expr:src="data:assetBase + &quot;/js/main.js&quot;"></script>
        <script>
            (function () {
                var homeRoot = document.getElementById('tg-home-root');
                if (!homeRoot) {
                    return;
                }

                var assetBase = "<data:assetBase/>";
                var homeUrl = "<data:blog.homepageUrl/>";
                var feedUrl = (homeUrl.charAt(homeUrl.length - 1) === '/' ? homeUrl : homeUrl + '/') + 'feeds/posts/default?alt=json&max-results=36';

                function escapeHtml(text) {
                    return String(text || '')
                        .replace(/&/g, '&amp;')
                        .replace(/</g, '&lt;')
                        .replace(/>/g, '&gt;')
                        .replace(/"/g, '&quot;')
                        .replace(/'/g, '&#39;');
                }

                function stripHtml(html) {
                    return String(html || '').replace(/<[^>]*>/g, ' ').replace(/\s+/g, ' ').trim();
                }

                function truncate(text, maxLen) {
                    var clean = String(text || '');
                    if (clean.length <= maxLen) {
                        return clean;
                    }
                    return clean.slice(0, maxLen).trim() + '...';
                }

                function formatDate(dateStr) {
                    if (!dateStr) {
                        return '';
                    }
                    var date = new Date(dateStr);
                    if (Number.isNaN(date.getTime())) {
                        return '';
                    }
                    return date.toLocaleDateString('id-ID', {
                        day: '2-digit',
                        month: 'short',
                        year: 'numeric'
                    });
                }

                function getLink(entry) {
                    if (!entry || !entry.link) {
                        return '#';
                    }
                    for (var i = 0; i < entry.link.length; i += 1) {
                        if (entry.link[i].rel === 'alternate') {
                            return entry.link[i].href;
                        }
                    }
                    return '#';
                }

                function getThumb(entry) {
                    var thumb = entry && entry.media$thumbnail ? entry.media$thumbnail.url : '';
                    if (!thumb) {
                        var html = (entry && entry.content && entry.content.$t) || '';
                        var match = html.match(/<img[^>]+src="([^"]+)"/i);
                        thumb = match ? match[1] : '';
                    }
                    if (!thumb) {
                        return assetBase + '/img/blog/blog01.jpg';
                    }
                    return thumb.replace(/\/s72-c\//, '/w820-h440-p/');
                }

                function mapEntry(entry) {
                    var title = entry && entry.title ? entry.title.$t : '';
                    var author = (entry && entry.author && entry.author.length) ? entry.author[0].name.$t : '';
                    var labels = entry && entry.category ? entry.category.map(function (c) { return c.term; }) : [];
                    var content = entry && entry.content ? entry.content.$t : (entry.summary ? entry.summary.$t : '');
                    var snippet = truncate(stripHtml(content), 140);
                    return {
                        title: title,
                        author: author,
                        labels: labels,
                        url: getLink(entry),
                        thumb: getThumb(entry),
                        date: formatDate(entry && entry.published ? entry.published.$t : ''),
                        snippet: snippet
                    };
                }

                function labelOrDefault(labels) {
                    return labels && labels.length ? labels[0] : 'news';
                }

                function renderBannerMain(post) {
                    return '' +
                        '<div class="tgbanner__thumb tgImage__hover">' +
                        '<a href="' + escapeHtml(post.url) + '"><img src="' + escapeHtml(post.thumb) + '" alt="img"></a>' +
                        '</div>' +
                        '<div class="tgbanner__content">' +
                        '<ul class="tgbanner__content-meta list-wrap">' +
                        '<li class="category"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(labelOrDefault(post.labels)) + '</a></li>' +
                        '<li><span class="by">By</span> <a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.author) + '</a></li>' +
                        '<li>' + escapeHtml(post.date) + '</li>' +
                        '</ul>' +
                        '<h2 class="title tgcommon__hover"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.title) + '</a></h2>' +
                        '</div>';
                }

                function renderBannerSide(post) {
                    return '' +
                        '<div class="tgbanner__post small-post">' +
                        '<div class="tgbanner__thumb tgImage__hover">' +
                        '<a href="' + escapeHtml(post.url) + '"><img src="' + escapeHtml(post.thumb) + '" alt="img"></a>' +
                        '</div>' +
                        '<div class="tgbanner__content">' +
                        '<ul class="tgbanner__content-meta list-wrap">' +
                        '<li class="category"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(labelOrDefault(post.labels)) + '</a></li>' +
                        '</ul>' +
                        '<h2 class="title tgcommon__hover"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.title) + '</a></h2>' +
                        '</div>' +
                        '</div>';
                }

                function renderTrending(post) {
                    return '' +
                        '<div class="swiper-slide">' +
                        '<div class="trending__post">' +
                        '<div class="trending__post-thumb tgImage__hover">' +
                        '<a href="' + escapeHtml(post.url) + '"><img src="' + escapeHtml(post.thumb) + '" alt="img"></a>' +
                        '</div>' +
                        '<div class="trending__post-content">' +
                        '<ul class="tgbanner__content-meta list-wrap">' +
                        '<li class="category"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(labelOrDefault(post.labels)) + '</a></li>' +
                        '<li><span class="by">By</span> <a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.author) + '</a></li>' +
                        '</ul>' +
                        '<h4 class="title tgcommon__hover"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.title) + '</a></h4>' +
                        '</div>' +
                        '</div>' +
                        '</div>';
                }

                function renderFeatured(post, index) {
                    return '' +
                        '<div class="col-lg-4 col-sm-6">' +
                        '<div class="featured__post">' +
                        '<div class="featured__thumb" style="background-image:url(' + escapeHtml(post.thumb) + ')">' + (index + 1) + '</div>' +
                        '<div class="featured__content">' +
                        '<ul class="tgbanner__content-meta list-wrap">' +
                        '<li class="category"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(labelOrDefault(post.labels)) + '</a></li>' +
                        '<li><span class="by">By</span> <a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.author) + '</a></li>' +
                        '</ul>' +
                        '<h4 class="title tgcommon__hover"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.title) + '</a></h4>' +
                        '</div>' +
                        '</div>' +
                        '</div>';
                }

                function renderVideoBig(post) {
                    return '' +
                        '<div class="video__post-item big-post">' +
                        '<div class="video__post-thumb">' +
                        '<a href="' + escapeHtml(post.url) + '"><img src="' + escapeHtml(post.thumb) + '" alt="img"></a>' +
                        '<a href="' + escapeHtml(post.url) + '" class="popup-video"><i class="fas fa-play"></i></a>' +
                        '</div>' +
                        '<div class="video__post-content">' +
                        '<ul class="tgbanner__content-meta list-wrap">' +
                        '<li class="category"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(labelOrDefault(post.labels)) + '</a></li>' +
                        '<li><span class="by">By</span> <a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.author) + '</a></li>' +
                        '<li>' + escapeHtml(post.date) + '</li>' +
                        '</ul>' +
                        '<h3 class="title tgcommon__hover"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.title) + '</a></h3>' +
                        '</div>' +
                        '</div>';
                }

                function renderVideoSide(post) {
                    return '' +
                        '<div class="video__post-item side-post">' +
                        '<div class="video__post-thumb tgImage__hover">' +
                        '<a href="' + escapeHtml(post.url) + '" class="popup-video"><img src="' + escapeHtml(post.thumb) + '" alt="img"><i class="fas fa-play"></i></a>' +
                        '</div>' +
                        '<div class="video__post-content">' +
                        '<ul class="tgbanner__content-meta list-wrap">' +
                        '<li class="category"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(labelOrDefault(post.labels)) + '</a></li>' +
                        '<li><span class="by">By</span> <a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.author) + '</a></li>' +
                        '</ul>' +
                        '<h3 class="title tgcommon__hover"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.title) + '</a></h3>' +
                        '</div>' +
                        '</div>';
                }

                function renderHandpicked(post) {
                    return '' +
                        '<div class="swiper-slide">' +
                        '<div class="trending__post">' +
                        '<div class="trending__post-thumb tgImage__hover">' +
                        '<a href="' + escapeHtml(post.url) + '"><img src="' + escapeHtml(post.thumb) + '" alt="img"></a>' +
                        '</div>' +
                        '<div class="trending__post-content">' +
                        '<ul class="tgbanner__content-meta list-wrap">' +
                        '<li class="category"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(labelOrDefault(post.labels)) + '</a></li>' +
                        '<li><span class="by">By</span> <a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.author) + '</a></li>' +
                        '</ul>' +
                        '<h4 class="title tgcommon__hover"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.title) + '</a></h4>' +
                        '</div>' +
                        '</div>' +
                        '</div>';
                }

                function renderStoriesTop(post) {
                    return '' +
                        '<div class="col-md-6">' +
                        '<div class="stories-post__item">' +
                        '<div class="stories-post__thumb tgImage__hover">' +
                        '<a href="' + escapeHtml(post.url) + '"><img src="' + escapeHtml(post.thumb) + '" alt="img"></a>' +
                        '</div>' +
                        '<div class="stories-post__content video__post-content">' +
                        '<ul class="tgbanner__content-meta list-wrap">' +
                        '<li class="category"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(labelOrDefault(post.labels)) + '</a></li>' +
                        '<li><span class="by">By</span> <a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.author) + '</a></li>' +
                        '<li>' + escapeHtml(post.date) + '</li>' +
                        '</ul>' +
                        '<h3 class="title tgcommon__hover"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.title) + '</a></h3>' +
                        '</div>' +
                        '</div>' +
                        '</div>';
                }

                function renderStoriesSmall(post) {
                    return '' +
                        '<div class="col-xl-3 col-lg-4 col-md-6">' +
                        '<div class="trending__post stories-small-post__item">' +
                        '<div class="trending__post-thumb tgImage__hover">' +
                        '<a href="' + escapeHtml(post.url) + '"><img src="' + escapeHtml(post.thumb) + '" alt="img"></a>' +
                        '</div>' +
                        '<div class="trending__post-content">' +
                        '<ul class="tgbanner__content-meta list-wrap">' +
                        '<li class="category"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(labelOrDefault(post.labels)) + '</a></li>' +
                        '<li><span class="by">By</span> <a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.author) + '</a></li>' +
                        '</ul>' +
                        '<h4 class="title tgcommon__hover"><a href="' + escapeHtml(post.url) + '">' + escapeHtml(post.title) + '</a></h4>' +
                        '</div>' +
                        '</div>' +
                        '</div>';
                }

                function setHtml(id, html) {
                    var el = document.getElementById(id);
                    if (el) {
                        el.innerHTML = html;
                    }
                }

                function fetchAndRender() {
                    fetch(feedUrl)
                        .then(function (response) { return response.json(); })
                        .then(function (data) {
                            var entries = data && data.feed && data.feed.entry ? data.feed.entry : [];
                            var posts = entries.map(mapEntry);
                            if (!posts.length) {
                                return;
                            }

                            var bannerMain = posts.slice(0, 1);
                            var bannerSide = posts.slice(1, 3);
                            var trending = posts.slice(3, 9);
                            var featured = posts.slice(9, 15);
                            var video = posts.slice(15, 20);
                            var handpicked = posts.slice(20, 26);
                            var storiesTop = posts.slice(26, 28);
                            var storiesSmall = posts.slice(28, 32);

                            if (bannerMain.length) {
                                setHtml('tg-banner-main', renderBannerMain(bannerMain[0]));
                            }
                            setHtml('tg-banner-side', bannerSide.map(renderBannerSide).join(''));
                            setHtml('tg-trending-wrapper', trending.map(renderTrending).join(''));
                            setHtml('tg-featured-grid', featured.map(renderFeatured).join(''));
                            if (video.length) {
                                setHtml('tg-video-big', renderVideoBig(video[0]));
                                setHtml('tg-video-side', video.slice(1).map(renderVideoSide).join(''));
                            }
                            setHtml('tg-handpicked-wrapper', handpicked.map(renderHandpicked).join(''));
                            setHtml('tg-stories-top', storiesTop.map(renderStoriesTop).join(''));
                            setHtml('tg-stories-small', storiesSmall.map(renderStoriesSmall).join(''));

                            if (window.TG_INIT_SWIPERS) {
                                window.TG_INIT_SWIPERS();
                            }
                        })
                        .catch(function () {
                            // Keep layout stable if feed fails.
                        });
                }

                fetchAndRender();
            })();
        </script>
    </b:with>
</body>
</html>
